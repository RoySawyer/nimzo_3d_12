msgid "PGN filename for player games"
msgstr "प्लेयर गेम्स के लिए PGN फ़ाइल नाम"

#: main_12.cpp:1680
msgid "Select .pgn file for the game"
msgstr "गेम के लिए .pgn फ़ाइल चुनें"

#: main_12.cpp:1715
msgid "Engine opponent"
msgstr "इंजन प्रतिद्वंद्वी"

#: main_12.cpp:1739
msgid "Engine ELO"
msgstr "इंजन ईएलओ"

#: main_12.cpp:1782
msgid "Personalities"
msgstr "व्यक्तित्व"

#: main_12.cpp:1802
msgid "Current player"
msgstr "वर्तमान खिलाड़ी"

#: main_12.cpp:1897
msgid "Player ELO"
msgstr "खिलाड़ी ELO"

#: main_12.cpp:1918
msgid "Edit player ELO rating"
msgstr "खिलाड़ी ELO रेटिंग संपादित करें"

#: main_12.cpp:1918
msgid "Check to edit player ELO rating"
msgstr "खिलाड़ी ELO रेटिंग संपादित करने के लिए जाँच करें"

#: main_12.cpp:1944
msgid "Set as current engine"
msgstr "वर्तमान इंजन के रूप में सेट करें"

#: main_12.cpp:1957
msgid "Set as current clock"
msgstr "वर्तमान घड़ी के रूप में सेट करें"

